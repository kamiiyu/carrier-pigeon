<tr class="<%= cycle(" odd", " even") -%>">
    <td><%= link_to event.name, event_path(event), class: 'btn btn-outline btn-link' %></td>
    <td><%= link_to event.started_at || '-', event_path(event), class: 'btn btn-outline btn-link' %></td>
    <td><%= link_to event.ended_at || '-', event_path(event), class: 'btn btn-outline btn-link' %> </td>
    <td><%= link_to event.event_type || '-', event_path(event), class: 'btn btn-outline btn-link' %> </td>
</tr>
